<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.codeeyes.mojito.mapper.ImgListMapper">


  <select id="getImgByName" resultType="cn.codeeyes.mojito.model.entity.ImgListDO">
